   "source": [
    "def count(s):\n",
    "    upper_count = sum(1 for c in s if c.isupper())\n",
    "    lower_count = sum(1 for c in s if c.islower())\n",
    "    return upper_count, lower_count\n",
    "\n",
    "inp = input()\n",
    "upper, lower = count(inp)\n",
    "print(\"Uppercase:\", upper)\n",
    "print(\"Lowercase:\", lower)\n"
   ]
  }
